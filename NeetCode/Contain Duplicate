var containsDuplicate = function(nums) {
    let set = new Set();
    for(let n of nums){
        if(set.has(n)){
            return true;
        }
        set.add(n);
    }

    return false;
};

/*
1. Create a new Set (a Set can only contain unique values)
2. Iterate over the nums array
3. If a number already exists in the Set, it is a duplicate → return true
4. If the loop finishes with no duplicates found → return false
=> time complexity is O(n)
*/
